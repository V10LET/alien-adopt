<h1>Welcome to Alien Adoption!</h1>

<div class="form">
  <div class="alien-carousel">
  <%@aliens.each do |alien|%>
    <div class="alien-parent">
      <div><a href="/aliens/<%=alien.id%>"><img class="alien-picture" src="/assets/<%=alien.name%>.png" alt="<%=alien.name%>"/></a></div>
      <%= link_to alien.name, alien_path(alien)%>
    </div>
  <%end%>
</div>
</div>
