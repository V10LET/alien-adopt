<%= form_with model: @alien do |alien| %>
  <%= form.text_field :name %>
  <%= form.text_area :bio%>
  <%= form.radio_button :therapy, checked: true, false%>
  <%= form.collection_select :toy_id, Toy.all, :id, :name%>
  <%= form.collection_select :planet_id, Planet.all, :id, :name %>
  <%= form.submit %>
<% end %>